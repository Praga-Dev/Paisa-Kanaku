@model Response<List<TransportModeInfo>>

<div class="form-group">
    <label class="form-label">
        Transport Mode
        <span class="required">*</span>
    </label>
    <select id="selectTransportMode" name="transportMode" class="form-control form-control-sm">
        @if (Model != null && Model.Data != null && Model.Data.Any())
        {
            @foreach (var transportMode in Model.Data)
            {
                <option value="@transportMode.TransportMode">@transportMode.TransportModeValue</option>
            }
        }
    </select>
</div>
