@model Response<List<ConsumerTypeInfo>>

<div class="form-group">
    <label class="form-label">
        Consumer Type
        <span class="required">*</span>
    </label>
    <select id="selectConsumerType" name="consumerType" class="form-control form-control-sm">
        @if (Model != null && Model.Data != null && Model.Data.Any())
        {
            @foreach (var consumerType in Model.Data)
            {
                <option value="@consumerType.ConsumerType">@consumerType.ConsumerTypeValue</option>
            }
        }
    </select>
</div>
