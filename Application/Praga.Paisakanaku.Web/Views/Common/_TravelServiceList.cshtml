@model Response<List<TravelServiceInfo>>

<div class="form-group">
    <label class="form-label">
        Travel Service
        <span class="required">*</span>
    </label>
    <select id="selectTravelService" name="travelService" class="form-control form-control-sm">
        @if (Model != null && Model.Data != null && Model.Data.Any())
        {
            @foreach (var travelService in Model.Data)
            {
                <option value="@travelService.TravelService">@travelService.TravelServiceValue</option>
            }
        }
    </select>
</div>
