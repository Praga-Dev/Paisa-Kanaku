@model Response<List<TimePeriodTypeInfo>>

<div class="form-group">
    <label class="form-label">
        TimePeriod
        <span class="required">*</span>
    </label>
    <select id="selectTimePeriod" name="timePeriod" class="form-control form-control-sm">
        @if (Model != null && Model.Data != null && Model.Data.Any())
        {
            @foreach (var timePeriod in Model.Data)
            {
                <option value="@timePeriod.TimePeriodType">@timePeriod.TimePeriodTypeValue</option>
            }
        }
    </select>
</div>
