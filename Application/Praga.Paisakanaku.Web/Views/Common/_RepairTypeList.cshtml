@model Response<List<RepairTypeInfo>>

<div class="form-group">
    <label class="form-label">
        Product
        <span class="required">*</span>
    </label>
    <select id="selectrepairType" name="repairType" class="form-control form-control-sm">
        @if (Model != null && Model.Data != null && Model.Data.Any())
        {
            @foreach (var repairType in Model.Data)
            {
                <option value="@repairType.Id">@repairType.Name</option>
            }
        }
        <option value="NEW">New</option>
    </select>
</div>